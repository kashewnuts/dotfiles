# -*- encoding: utf-8 -*-
# Python startup
try:
    import readline
    import rlcompleter
    import atexit
    import os
    import platform
    # Tab complement
    readline.parse_and_bind('tab: complete')
    # History File
    histfile = os.path.join(os.environ['HOME'], '.pythonhistory')

    try:
        readline.read_history_file(histfile)
    except IOError:
        pass
    atexit.register(readline.write_history_file, histfile)
    del platform, os, histfile, environ_path, readline, rlcompleter, atexit
except:
    pass
